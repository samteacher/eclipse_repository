<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<!DOCTYPE HTML>
<html>
 <head>
	<title>登陆页面-JavaWeb学习</title>
	<meta charset="utf-8">
	<link href="css/style.css" rel='stylesheet' type='text/css' />
	<meta name="viewport" content="width=device-width, initial-scale=1">
</head>
<body>
	 <!-----start-main---->
	 <div class="main">
		<div class="login-form">
			<h1>KeKe个人博客登陆</h1>
				<div class="head">
					<img src="images/user.png" alt=""/>
				</div>
				<form method="post" action="sessicon">
					<input type="text" name="username" class="text" placeholder="请输入用户名" >
					<input type="password" name="password" placeholder="请输入密码">
					<div class="submit">
					<input type="submit" onclick="myFunction()" value="登陆" >
					<select name="coursename">
						<option value="java">java</option>
						<option value="c#">c#</option>
						<option value="php">php</option>
						<option value="c++">c++</option>
					</select>
					</div>	
					<p><a href="#">忘记密码 ?</a></p>
				</form>
			</div>
			<!--//End-login-form-->
			 <!-----start-copyright---->
   					<div class="copy-right">
						<p>Copyright &copy; 2014.Company 学院 All rights reserved.</p> 
					</div>
				<!-----//end-copyright---->
		</div>
			 <!-----//end-main---->
		 		
</body>
</html>
